{
  "name": "gym",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node build/server.js",
    "dev": "yarn svcs:up && yarn svcs:wait:database && yarn run-migrations && ts-node-dev src/server.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli -f ./ormconfig.js",
    "build": "tsc --build && npx typeorm migration:run",
    "run-migrations": "npm run typeorm migration:run",
    "create-database": "psql -U postgres -f src/database/create_database.sql && npm run run-migrations",
    "svcs:up": "docker compose -f src/database/compose.yaml up -d",
    "svcs:stop": "docker compose -f src/database/compose.yaml stop",
    "svcs:down": "docker compose -f src/database/compose.yaml down",
    "svcs:wait:database": "node src/database/script/wait-for-postgres"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^16.4.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "cookie": "^1.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^12.0.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.7.7",
    "pg": "^8.6.0",
    "random-js": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "typeorm": "^0.2.34",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/pg": "^8.6.5",
    "@types/uuid": "^8.3.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.3.5"
  }
}
